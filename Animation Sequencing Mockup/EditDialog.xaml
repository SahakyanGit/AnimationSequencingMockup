<Window x:Class="Animation_Sequencing_Mockup.EditDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Animation_Sequencing_Mockup"
        mc:Ignorable="d"
        Title="EditDialog" Height="160.248" Width="310.248">
    <Grid>
        <Grid.RowDefinitions >
            <RowDefinition Height="2*" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <TextBox Text="Name" x:Name="name" Grid.Row="0" VerticalAlignment="Center" />
        <WrapPanel Grid.Row="1" HorizontalAlignment="Right">
            <Button VerticalAlignment="Center" Content="OK" Click="OK_Click" />
            <Button VerticalAlignment="Center" Content="Cancel" Click="Cancel_Click" />
        </WrapPanel>
    </Grid>
</Window>
